{
  "name": "aide-checklist",
  "displayName": "AIDE Checklist",
  "description": "Create and manage AIDE_CHECKLIST.md with a hidden .aide brain.",
  "version": "0.0.2",
  "publisher": "aide",
  "license": "MIT",
  "private": true,
  "engines": {
    "vscode": "^1.108.0"
  },
  "main": "./src/extension.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aide.checklist.init",
    "onCommand:aide.checklist.open",
    "onCommand:aide.checklist.sync",
    "onCommand:aide.checklist.markDone",
    "onCommand:aide.checklist.snapshot"
  ],
  "contributes": {
    "commands": [
      {
        "command": "aide.checklist.init",
        "title": "AIDE: Init Checklist"
      },
      {
        "command": "aide.checklist.open",
        "title": "AIDE: Open Checklist"
      },
      {
        "command": "aide.checklist.sync",
        "title": "AIDE: Sync Checklist"
      },
      {
        "command": "aide.checklist.markDone",
        "title": "AIDE: Mark Task Done"
      },
      {
        "command": "aide.checklist.snapshot",
        "title": "AIDE: Capture Snapshot"
      }
    ]
  }
}
